  push bc
  th.with.ret 0
endsub th.push



defsub th.loop
 ; hook before stepall
 db 0cdh
 h.thent:
 dw int.nop
 ; save prev timecnt
 ld a,(timecnt)
 push af
 ; Do stepall
 call th.stepall
 ; hook after stepall
 db 0cdh
 h.thlop:
 dw int.nop
 ; wait until timecnt changes
 pop af
 bwat:
  ld hl,timecnt
  cp (hl)
  jr nz,bbwat
  push af
  call doint
  pop af
  jr bwat
 bbwat:
 ; repeat
 jr th.loop
endsub th.loop

