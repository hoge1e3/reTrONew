        z_romboot=` org 04000h
 db 41h, 42h
 dw rom.init
 ds 12,0
rom.init:
 call cpslt
 jp _main     

;include z_ctrl

rom.ENASLT EQU 0024H
rom.RSLREG EQU 0138H
rom.EXPTBL EQU 0FCC1H


rdslt:
 ex de,hl
 ;DE=PGxx xxxx xxxx xxxx
 rept 5
  srl d;page*2
 endm
 ;DE=xxxx xPGx xxxx xxxx
 CALL rom.RSLREG
 ;A = PRIMARY_SLOT
 ld e,d
 ;A >>= PG*2
rom.rdslt1:
  RRCA
  dec e
  jp nz,rom.rdslt1
 AND    00000011B
 LD c,A;000000Pr
 LD B,0
 LD HL,rom.EXPTBL
 ADD HL,BC
 LD c,A;000000Pr
 LD A,(HL)
 AND 80H;expand flag
 OR c
 bit 7,a
 ret z
 LD c,A;e00000Pr
 rept 4
  INC HL
 endm
 LD A,(HL);exp reg
 ld e,d
rom.rdslt2:
  srl a
  dec e
  jp nz,rom.rdslt2
;000000Ex
 sla a
 sla a
;0000Ex00
 and  00001100b
 OR c;e000ExPr
 ret

cpwork equ 0c000h
cplen equ 01000h
rom equ cpwork+cplen
ram equ rom+1
ramtop equ 8000h

cpslt:
 ld hl,4000h
 call rdslt
 ld (rom),a
 ld hl,cpwork
 call rdslt
 ld (ram),a
 ld hl,rom
 cp (hl)
 ret z
 ld hl,ramtop 
cpslt.loop:
  push hl
  ld a,(rom)
  ld hl,ramtop 
  call rom.ENASLT
  pop hl
  push hl
  ld bc,cplen
  ld de,cpwork
  ldir
  ld a,(ram)
  ld hl,ramtop 
  call rom.ENASLT
  ld hl,cpwork
  pop de
  ld bc,cplen
  ldir
  ex de,hl
  ld a,h
  cp 0c0h
  jr c,cpslt.loop
 ret

