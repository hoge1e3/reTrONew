defsub th.isblank.a
 ; h= thread
 ; z if true
 ld l, th.ofs.stp
 ld a,(hl)
 cp th.st.blank
endsub th.isblank.a

defwork th.new.start
 db th.start.h
th.new.end:
 db th.end.h
endwork th.new.start

defsub th.find.blank
; h: result  nz: not found
#local
 ld a,(th.new.end)
 ld b,a
 ld a,(th.new.start)
 ld h,a
 ld l,th.ofs.stp
loop:
 ld a,(hl)
 cp th.st.blank
 ret z
 ld a,b
 inc h
 cp h
 jr nz,loop
 inc a; reset z
#endlocal
endsub 0

defsub th.new
; nz for alloc fail
#local
 call th.find.blank
 ret nz
 ; TODO th.ofs.stp
 ld L,th.ofs.stp
 ld (HL),31h
 inc HL
 ld (HL),th.ofs.spini
 ld a,h
 inc HL
 ld (hl),a
 inc HL
 ld (HL),0c9h
 ld l,th.bottom
 ld a,l
 cp l; set z
 ret
#endlocal
endsub th.new

defsub th.init
#local
