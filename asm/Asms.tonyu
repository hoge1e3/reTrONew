defsub o.new
#local
 ; {val .f} n &initbl retad
 pop hl;retad
 ld (retad-2),hl
 ; set initbl for th.with
 pop hl;&initbl
 th.with.setdst hl
 ; save this
 ld (svthis-1),a
 ; allocate thread
 call th.new
 jr nz, allocfail
 push hl; thread address
 call th.with.s; call &initbl
 pop hl; thread address
 ld a,h; set this as thread
 ; init fields
 pop bc; n of {val .f}
 inc c
 loop:
  dec c
  jr z,lpend
  pop hl; .f
  ld h,a
  ld (w-2),hl
  pop hl; val
  ld (w),hl
  srlabel w
 jr loop
 lpend:
 ; return h as this
 ld h,a
 finally:
  ;restore a before call o.new
  ld a,0
  srlabel svthis
  ;ret 
  jp 0
  srlabel retad
 allocfail:
  ; drop {val .f}
  pop bc; n of {val .f}
  ld b,c
  inc c
  lp2:
   dec c
   jr z, lp2end
   pop hl
   pop hl
  jr lp2
  lp2end:
  ld hl,null;  todo null
  jr finally
#endlocal
endsub o.new
