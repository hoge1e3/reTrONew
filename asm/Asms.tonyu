defsub clamp
#local
 ; hl value
 ; bc min
 ; de max
 push hl
 subhl bc
 bit 7,h
 pop hl
 jr z, notmin
  ld h,b
  ld l,c
  ret
 notmin:
 push hl
 subhl de
 bit 7,h
 pop hl
 ret nz
 ex de,hl
 ret
#endlocal
endsub clamp

defsub mul.a
