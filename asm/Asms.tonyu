defsub th.with.s
; with(this) { init routine } 
;  init routine should end with th.with.ret
; h -> this
; th.with.setdst <addr> -> init routine address
 ld (th.wrssp-2),sp
 ld l, th.ofs.sp
 ld (th.updsp-2),hl
 mem2sp 0
 jp 0
srlabel th.jpdest
th.ewith:
 ld (0),sp
srlabel th.updsp
 ld sp,0
srlabel th.wrssp
endsub th.with.s





defsub th.push
 ;push bc to thread hl
 th.with tpsbc
 ret
